<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:annotation>
    <xs:documentation>
      * Filename   : config.xsd
      * Description: XML Schema for BioNLP
      * Author(s)  : Chen Li
      * Revision   : $Id: config.xsd 8275 2013-10-16 16:28:25Z $
